<%= form_for @reminder do |f| %>
<%= f.label :what %>
<%= f.text_field :what %>
<%= f.label :where %>
<%= f.text_field :where %>
<%= f.label :time %>
<%= f.text_field :time %>
<%= f.label :tribe_id %>
<%= f.collection_select :tribe_id, @tribes, :id, :name, :prompt => true %>
<%= f.submit %>
<% end %>
